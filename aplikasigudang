            JOptionPane.showMessageDialog(null, "terdapat data yang masih kosong", "Informasi", JOptionPane.INFORMATION_MESSAGE);
        } else {
            jTextArea1.setText(g.searchBarang(namaBarangEdit.getText()));
        }
    }//GEN-LAST:event_jButton3ActionPerformed

    private void jButton4ActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_jButton4ActionPerformed
        // TODO add your handling code here:
        if ((namaBarangSearch.getText().equals(""))) {
            JOptionPane.showMessageDialog(null, "terdapat data yang masih kosong", "Informasi", JOptionPane.INFORMATION_MESSAGE);
        } else {
            String cari = namaBarangSearch.getText();
            jTextArea2.setText(g.searchBarang(cari));
        }
    }//GEN-LAST:event_jButton4ActionPerformed

