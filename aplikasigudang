package Tubes_Gudang;

public class Gudang implements Method {

    private String nama;
    private int totalBarang = 0;
    private Barang[] b = new Barang[50];
    private Pegawai[] p = new Pegawai[100];
    private int totalPegawai = 0;
    private int i;
    private String s;
    public Gudang (){
        
    }

    public Gudang(String nama) {
        this.nama = nama;
    }

    /**
     *
     * @param cari
     * @return
     */
    public int getTotalBarang() {
        int i;
        int tb = 0;
        for (i = 0; i < totalBarang; i++) {
            tb = tb + b[i].getJumlah();
        }
        return tb;
    }

    public String getNama() {
        return nama;
    }

    @Override
    public String toString() {
        return "Nama Gudang : " + getNama() + "\nTotal Barang : " + getTotalBarang();
    }

    @Override
    public void stringaddBarang2() {
        try {
            for (int i = 0; i < totalBarang; i++) {
                String s = "";
                s = s + " \n " + b[i].toString();
                System.out.println(s);
            }
        } catch (Exception e) {
            System.out.println("Tidak ada barang dalam gudang ini");
        }

    }

    public Barang[] getBarang() {
        return b;
    }

    @Override
    public int getJumlahBarang() {
        return totalBarang; //To change body of generated methods, choose Tools | Templates.
    }
    
    //Menu Barang

    public String searchBarang(String cari) {
        String s = "";
        int i = 0;
        do {
            if (b[i].getNama().equals(cari)) {
                //System.out.println(b[i].toString());
                s = s + b[i].toString();
            } else {
                s = s;
            }
            i++;
        } while (i < totalBarang);
        try {
            if (s == "") {
                throw new Exception("tidak ada");
            }
        } catch (Exception e) {
            System.out.println("Hasil tidak dapat ditemukan");
        }
        return s;
    }

    @Override
    public void editBarang(String cari,String nama, int jum) {
        try {
            int i;
            for (i = 0; i < totalBarang; i++) {
                if (b[i].getNama().equals(cari)) {
                    b[i].setNama(nama);
                    b[i].setJumlah(jum);
                    System.out.println("\nUpdate berhasil");
                }
//                throw new FileNotFoundException("Hasil ditemukan");
            }
            
        } catch (Exception e) {
            System.out.println("Update gagal");
        }

    }

    public void barangMasuk(Barang b) {
        this.b[totalBarang] = b;
        totalBarang++;
    }
    public void barangOut(String cari){
        int i;
        try {
            for (i = 0; i < totalPegawai; i++) {
                if (b[i].getNama().equals(cari)) {
                    b[i].setStatus("out");
                    System.out.println("\nBarang berhasil dikirim");
                }
            }
        } catch (Exception e) {
            System.out.println("\nBarang gagal dikirim");
        }
        
    }
    
    //Menu Pegawai

    @Override
    public void addPegawai(Pegawai p) {
        this.p[totalPegawai] = p;
        totalPegawai++;
    }

    @Override
    public String searchPegawai(String cari) {
        String s = "";
        int i;
        try {
            for (i = 0; i < totalPegawai; i++) {
                if (p[i].getNama().equals(cari)) {
                    s = s + p[i].toString();
                }
            }
        } catch (Exception e) {
            System.out.println("\nHasil tidak dapat ditemukan");
        }
        return s;
    }

    @Override
    public void setPJ(String barang, String pegawai) {
        String barangCari = "", pegawaiCari = "";
        int i, j, bfix = 0, pfix = 0;
        try {
            for (i = 0; i < totalBarang; i++) {
                if (b[i].getNama().equals(barang)) {
                    b[bfix] = b[i];
                }
            }
            for (j = 0; j < totalPegawai; j++) {
                if (p[j].getNama().equals(barang)) {
                    p[pfix] = p[j];
                }
            }
            b[bfix].setPJ(p[pfix]);
            System.out.println("\n"+p[pfix].getNama() + " menjadi penanggung jawab " + b[bfix].getNama());
        } catch (Exception e) {
            System.out.println("\nTransaksi gagal");
        }
    }

    @Override
    public void removePegawai(String nama) {
        int i;
        try {
            for (i = 0; i < totalPegawai; i++) {
                if (p[i].getNama().equals(nama)) {
                    p[i]=null;
                }
            }
        } catch (Exception e) {
            System.out.println("\nHasil tidak dapat ditemukan");
        }
    }
    public void logBook(){
        
    }

    @Override
    public String searchPerusahaan(String cari) {
        String s = "";
        int i;
        try {
            for (i = 0; i < totalPegawai; i++) {
                if (b[i].getNamaPerusahaan().equals(cari)) {
                    s = b[i].toString();
                }
            }
            System.out.println("\nHasil dari pencarian ");
        } catch (Exception e) {
            System.out.println("\nHasil tidak dapat ditemukan");
        }
        return s;
    }
    
}

package Tubes_Gudang;

import java.util.Scanner;

public class Driver {

    public static void main(String[] args) {
        GUIfix gui = new GUIfix();
        gui.setVisible(true);
    }
}

package Tubes_Gudang;
public class Barang {

    private String nama, jenis,status="in";
    private int jumlah;
    private PerusahaanSupplier perusahaan;
    private Pegawai PJ;
    public Barang(){
        
    }

    public Barang(String nama, String jenis, int jumlah, String namaPerusahaan) {
        try {
            this.nama = nama;
            this.jenis = jenis;
            this.jumlah = jumlah;
            perusahaan.setNama(namaPerusahaan);
            status="in";
            throw new IllegalArgumentException("Inputan benar");
        }catch (IllegalArgumentException e){
            System.out.println();
        }

    }

    public void setNama(String nama) {
        this.nama = nama;
    }

    public void setJenis(String jenis) {
        this.jenis = jenis;
    }

    public void setJumlah(int jumlah) {
        this.jumlah = jumlah;
    }
    
    public String getNama() {
        return nama;
    }

    public String getJenis() {
        return jenis;
    }

    public int getJumlah() {
        return jumlah;
    }

    public void setNamaPerusahaan(String nama) {
        perusahaan = new PerusahaanSupplier(nama);
        //perusahaan.setNama(nama);
    }

    public String getNamaPerusahaan() {
        return perusahaan.getNama();
    }

    @Override
    public String toString() {
        return "\nNama Barang : " + getNama() + "\nJumlah Barang : "+getJumlah()+"\nJenis Barang : " + getJenis() + "\nNama perusahaan : " + getNamaPerusahaan()+ "\nNama Pegawai Penanggung Jawab: " + getNamaPJ()+"\nStatus: "+getStatus();
    }
    public String toStringPJ(){
        return "\nNama Barang : " + getNama() + "\nJumlah Barang : "+getJumlah()+"\nJenis Barang : " + getJenis() + "\nNama perusahaan : " + getNamaPerusahaan()+ "\nNama Pegawai Penanggung Jawab: "+"\nStatus: "+getStatus();
    }

    public void setPJ(Pegawai p) {
        PJ=p;
    }

    public String getNamaPJ() {
        return PJ.getNama();
    }
    public String getStatus(){
        return status;
    }
    public void setStatus(String s){
        status=s;
    }
}

public class Pegawai{
	private String nama,kodePegawai;
	private Barang b;
	public Pegawai (String nama, String kodePegawai){
		this.nama=nama;
		this. kodePegawai=kodePegawai;
	}
	public String getNama(){
		return nama;
	}
	public String getKodePegawai(){
		return kodePegawai;
	}
	public String toString(){
		return "/nNama Pegawai : "+getNama()+"/nNIP : "+kodePegawai;
	}
}
