package Tubes_Gudang;

public class PerusahaanSupplier {

    private String nama;
    private int kode=11000;

    public PerusahaanSupplier(String nama) {
        try {
            this.nama = nama;
            kode++;
        } catch (Exception e) {
            System.out.println("Inputan Gagal");
        }
    }
    

    public String getNama() {
        return nama;
    }

    public int getKode() {
        return kode;
    }

    public void setNama(String nama) {
        this.nama = nama;
    }
}

package Tubes_Gudang;

public class Pegawai {

    private String nama, kodePegawai;
    private Barang b;
    public Pegawai(){
        
    }

    public Pegawai(String nama, String kodePegawai){
        this.nama = nama;
        this.kodePegawai = kodePegawai;
        try {
            if (kodePegawai.length() != 5) {
                throw new IllegalArgumentException("Kode pegawain 5 digit");
            }
        } catch (IllegalArgumentException e) {
            System.out.println("Kode pegawai tidak 5 digit");
        }
    }

    public String getNama() {
        return nama;
    }

    public String getKodePegawai() {
        return kodePegawai;
    }

    public String toString() {
        return "\nNama Pegawai : " + getNama() + "\nNIP : " + kodePegawai;
    }

    public void setNama(String nama) {
        this.nama = nama;
    }

    public void setKodePegawai(String kodePegawai) {
        this.kodePegawai = kodePegawai;
    }
    
}

