    private void jButton9ActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_jButton9ActionPerformed
        if ((namaBarangEditBaru.getText().equals("")) || (jumlahBarangEditBaru.getText().equals(""))) {
            JOptionPane.showMessageDialog(null, "terdapat data yang masih kosong", "Informasi", JOptionPane.INFORMATION_MESSAGE);
        } else {
            g.editBarang(g.searchBarang(namaBarangEdit.getText()), namaBarangEditBaru.getText(), Integer.parseInt(jumlahBarangMasuk.getText()));
        jTextArea1.setText(g.searchBarang(namaBarangEditBaru.getText()));
        logBook=logBook+"\nAda barang yang diedit bernama "+namaBarangEditBaru.getText();
        }
        // TODO add your handling code here:
    }//GEN-LAST:event_jButton9ActionPerformed

