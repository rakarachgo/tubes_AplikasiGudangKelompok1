    private void jButton7ActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_jButton7ActionPerformed
        // TODO add your handling code here:
        if ((namaBarangPJ.getText().equals("")) || (namaPegawaiPJ.getText().equals(""))) {
            JOptionPane.showMessageDialog(null, "terdapat data yang masih kosong", "Informasi", JOptionPane.INFORMATION_MESSAGE);
        } else {
            g.setPJ(namaBarangPJ.getText(), namaPegawaiPJ.getText());
            jTextArea3.setText(namaPegawaiPJ.getText() + " menjadi penanggung jawab " + namaBarangPJ.getText());
        }
    }//GEN-LAST:event_jButton7ActionPerformed

    private void jButton8ActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_jButton8ActionPerformed
        // TODO add your handling code here:
        if (namaSearchPegawai.getText().equals("")) {
            JOptionPane.showMessageDialog(null, "terdapat data yang masih kosong", "Informasi", JOptionPane.INFORMATION_MESSAGE);
        } else {
            jTextArea4.setText(g.searchPegawai(namaSearchPegawai.getText()));
        }
    }//GEN-LAST:event_jButton8ActionPerformed

    private void jButton12ActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_jButton12ActionPerformed
        // TODO add your handling code here:
        if ((namaPerusahaanSearch.getText().equals(""))) {
            JOptionPane.showMessageDialog(null, "terdapat data yang masih kosong", "Informasi", JOptionPane.INFORMATION_MESSAGE);
        } else {
            jTextArea6.setText(g.searchPerusahaan(namaPerusahaanSearch.getText()));
        }
    }//GEN-LAST:event_jButton12ActionPerformed

    private void jButton5ActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_jButton5ActionPerformed
        // TODO add your handling code here:
        if ((nama.getText().equals("")) || (kode.getText().equals("")) || (kode.getText().length() != 5)) {
            JOptionPane.showMessageDialog(null, "terdapat data yang masih kosong atau kode pegawai salah", "Informasi", JOptionPane.INFORMATION_MESSAGE);
        } else {
            p.setNama(nama.getText());
            p.setKodePegawai(kode.getText());
            g.addPegawai(p);
            JOptionPane.showMessageDialog(null, "Data Berhasil di Input", "Informasi", JOptionPane.INFORMATION_MESSAGE);
            
        }
    }//GEN-LAST:event_jButton5ActionPerformed

    private void jButton6ActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_jButton6ActionPerformed
        // TODO add your handling code here:
        if (namaPegawaiRemove.getText().equals("")) {
            JOptionPane.showMessageDialog(null, "terdapat data yang masih kosong", "Informasi", JOptionPane.INFORMATION_MESSAGE);
        } else {
            g.removePegawai(namaPegawaiRemove.getText());
            JOptionPane.showMessageDialog(null, "data berhasil dihapus", "Informasi", JOptionPane.INFORMATION_MESSAGE);

        }
    }//GEN-LAST:event_jButton6ActionPerformed

    private void jButton1ActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_jButton1ActionPerformed
        // TODO add your handling code here:
        if ((namaBarangMasuk.getText().equals("")) || (jenisBarangMasuk.getText().equals("")) || (jumlahBarangMasuk.getText().equals("")) || (perusahaanBarangMasuk.getText().equals(""))) {
            JOptionPane.showMessageDialog(null, "terdapat data yang masih kosong", "Informasi", JOptionPane.INFORMATION_MESSAGE);
        } else {
            //brg = new Barang(namaBarang.getText(), jenisBarang.getText(), Integer.parseInt(jumlahBarang.getText()), namaPerusahaan.getText());
            int i;
            b.setNama(namaBarangMasuk.getText());
            b.setJenis(jenisBarangMasuk.getText());
            b.setJumlah(Integer.parseInt(jumlahBarangMasuk.getText()));
            b.setNamaPerusahaan(perusahaanBarangMasuk.getText());
            g.barangMasuk(b);
            logBook=logBook+"\nAda barang masuk bernama "+namaBarangMasuk.getText();

            JOptionPane.showMessageDialog(null, "Data Berhasil di Input", "Informasi", JOptionPane.INFORMATION_MESSAGE);
            
        }
    }//GEN-LAST:event_jButton1ActionPerformed

    private void jButton2ActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_jButton2ActionPerformed
        // TODO add your handling code here:
        if ((namaBarangKeluar.getText().equals(""))) {
            JOptionPane.showMessageDialog(null, "terdapat data yang masih kosong", "Informasi", JOptionPane.INFORMATION_MESSAGE);
        } else {
            g.barangOut(namaBarangKeluar.getText());
            logBook=logBook+"\nAda barang keluar bernama "+namaBarangKeluar.getText();
            JOptionPane.showMessageDialog(null, "Barang Berhasil dikirim", "Informasi", JOptionPane.INFORMATION_MESSAGE);
        }
    }//GEN-LAST:event_jButton2ActionPerformed

    private void jButton3ActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_jButton3ActionPerformed
        // TODO add your handling code here:
